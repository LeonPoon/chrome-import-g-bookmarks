{
  "name": "__MSG_extName__",
  "version": "2.1.4",
  "manifest_version": 2,
  "description": "Sync from Google Bookmarks",
  "permissions": [
    "https://www.google.com/",
    "bookmarks"
  ],
  "browser_action": {
    "default_icon": "bookmark-icon.png",
    "default_title": "Sync from Google Bookmarks"
  },
  "background": {
    "scripts": [
	"json2.js",
	"jquery.min.js", 
	"extension.js", 
	"bookmarks.js",
	"browser_action.js"]
  },
  "default_locale": "en",
	"icons": {"128": "bookmark-icon.png"}
}
